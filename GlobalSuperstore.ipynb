        "## Converting CSV to DB for use with SQLite"
      ],
      "metadata": {
        "id": "V3IT7df-4xuu"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "import pandas as pd\n",
        "import sqlite3\n",
        "\n",
        "df = pd.read_csv(\"superstore-MH.csv\")\n",
        "conn = sqlite3.connect(\"superstore.db\")\n",
        "df.to_sql(\"superstore\", conn, if_exists=\"replace\", index=False)\n",
        "conn.close()\n"
      ],
